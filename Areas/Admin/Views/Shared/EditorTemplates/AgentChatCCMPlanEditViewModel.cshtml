@using Cyara.Shared.Web.Extensions
@using Cyara.Web.Common.Extensions
@model Cyara.Web.Portal.Areas.Admin.Models.AgentChatCCMPlanEditViewModel

<div class="form-head">
    <h2>@Html.GlobalResource("PlanDetails")</h2>
</div>
<div class="form-body">       
    @Html.Partial("EditorTemplates/PlanEdit")
    <div class="control-group">
        @Html.LabelForWithRequired(m => m.Agents, new {@class = "control-label"})
        <div class="controls">
            @Html.TextBoxFor(m => m.Agents, new { @class = "input-mini" })
            @Html.ValidationMessageFor(m => m.Agents)
        </div>
    </div>
</div>
