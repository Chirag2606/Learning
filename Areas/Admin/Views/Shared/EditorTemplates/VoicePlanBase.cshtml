@using Cyara.Shared.Web.Extensions
@using Cyara.Web.Common.Extensions
@model Cyara.Web.Portal.Areas.Admin.Models.VoicePlanBase

<div class="control-group">
    @Html.LabelForWithRequired(m => m.Ports, new {@class = "control-label"})
    <div class="controls">
        @Html.TextBoxFor(m => m.Ports, new { @class = "input-mini" })
        @Html.Tooltip(Html.Resource("PortsTooltip"), Html.GlobalResource("TipLabel"))
        @Html.ValidationMessageFor(m => m.Ports)
    </div>
</div>